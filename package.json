{
  "name": "test_rover",
  "version": "1.0.0",
  "description": "Foxbit Backend Case",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|md)\"",
    "lint": "eslint --ext .js,.ts,.json --fix .",
    "dev": "nodemon --watch src src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters html",
    "build": "npm run format && npm run lint && tsc",
    "start": "npm run build && node dist/index.js"
  },
  "author": "igor alves dos santos",
  "devDependencies": {
    "@babel/preset-typescript": "7.24.1",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "^29.7.0",
    "nodemon": "3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
